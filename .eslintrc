{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            2,
            "single"
        ],
        "object-curly-spacing": [
            2,
            "always"
        ]
    }
}